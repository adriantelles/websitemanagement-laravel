{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "indent": ["error", 4],
        "camelcase": ["error", { "properties": "never" }]
    },
    "globals": {
        "Vue": true,
        "window": true,
        "_": true,
        "axios": true
    }
}